{
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.19",
        "localheinz/composer-normalize": "^2.28",
        "maglnet/composer-require-checker": "^4.1",
        "mi-schi/phpmd-extension": "^4.3",
        "phpmd/phpmd": "^2.12"
    },
    "config": {
        "allow-plugins": {
            "localheinz/composer-normalize": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
